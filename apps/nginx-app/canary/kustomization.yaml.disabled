apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization
namespace: easy-nginx

resources:
- ../../../k8s/namespace.yaml
- argo-rollout.yaml
- service.yaml
- service-canary.yaml

commonLabels:
  app.kubernetes.io/name: easy-nginx
  app.kubernetes.io/instance: easy-nginx
  app.kubernetes.io/version: "1.0.0"
  app.kubernetes.io/part-of: easy-nginx-app

images:
- name: mofwili/easy-nginx
  newTag: latest